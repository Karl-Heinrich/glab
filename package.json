{
  "name": "glab",
  "version": "0.0.1",
  "description": "An interactive gitlab-cli with the goal to completely avoid the webinterface",
  "main": "dist/index.js",
  "bin": "glab",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "npx tsc -p .",
    "test": "sudo npm i -g && glab"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karl-Heinrich/glab.git"
  },
  "keywords": [
    "gitlab-cli",
    "node",
    "cli",
    "gitlab",
    "api"
  ],
  "author": "Tobias Karl-Heinrich Gutmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karl-Heinrich/glab/issues"
  },
  "homepage": "https://github.com/Karl-Heinrich/glab#readme",
  "dependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^3.0.1",
    "figlet": "^1.2.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "ts-node": "^8.4.1",
    "@types/node": "^12.7.5",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.3"
  }
}
